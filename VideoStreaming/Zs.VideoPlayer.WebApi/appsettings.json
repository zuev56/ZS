{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:80"
      }
    }
  },

  "CorsPolicy": {
    "Origins": [
      "http://*:6002"
    ]
  },

  "AllowedHosts": "*",

  "VideosDirectory": "/var/lib/videos",
  "VideoFilesName": ".video"
}
