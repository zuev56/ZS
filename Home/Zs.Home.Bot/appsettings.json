{
  "Bot": {
    "Token": "",
    "Name": "",
    "OwnerChatRawId": 0,
    "CliPath": "/bin/bash",
    "PrivilegedUserRawIds": [ ]
  },

  "ConnectionStrings": {
    "Default": ""
  },

  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:6022"
      }
    }
  },

  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss.fff} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://192.168.1.104:8500" }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName"
    ],
    "Properties": {
      "ApplicationName": "DEV.HomeBot"
    }
  },

  "Seq": {
    "Url": "http://192.168.1.104:8500",
    "ApiKey": null,
    "MaxEventsPerRequest": 10000
  },

  "Notifier": {
    "FromHour": "8", "ToHour": "22"
  },

  "ConnectionAnalyser": {
    "Urls": [ "https://vk.com/", "https://yandex.ru/", "https://www.google.ru/" ]
  },

  "Proxy": {
    "UseProxy": false,
    "Socket": "address:port",
    "User": "",
    "Password": ""
  },

  "HomeClient": {
    "Url": "http://localhost:6024"
  },

  "UserWatcher": {
    "VkActivityApiUri": "http://localhost:6011",
    "TrackedIds": [ 8790237 ],
    "InactiveHoursLimit": 1
  }
}
