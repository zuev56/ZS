{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ConnectionStrings": {
    "Default": ""
  },

  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:6023"
      }
    }
  },

  "HomeBotUrl": "http://localhost:6022",

  "UserWatcher": {
    "CronExpression": "*/10 * * * *",
    "VkActivityApiUri": "http://localhost:6011",
    "TrackedIds": [ 8790237 ],
    "InactiveHoursLimit": 1
  },

  "WeatherAnalyzer": {
    "CronExpression": "*/5 * * * *",
    "Devices": [
      {
        "Uri": "http://192.168.1.120/",
        "Name": "ESP Meteo",
        "Sensors": [
          {
            "Name": "DHT11/22 1",
            "Alias" : "ServerCabinet",
            "Parameters": [
              { "Name": "Temperature", "LoLo": 12, "Lo": 15, "Hi": 26, "HiHi": 28 },
              { "Name": "Humidity", "LoLo": 30, "Lo": 35, "Hi": 55, "HiHi": 60 }
            ]
          },
          {
            "Name": "DHT11/22 2",
            "Alias" : "Bedroom",
            "Parameters": [
              { "Name": "Temperature", "LoLo": 18, "Lo": 20, "Hi": 23, "HiHi": 25 },
              { "Name": "Humidity", "LoLo": 30, "Lo": 35, "Hi": 55, "HiHi": 60 }
            ]
          }
        ]
      }
    ]
  },

  "WeatherRegistrator": {
    "CronExpression": "*/10 * * * *",
    "Places": [
      { "Id": 1, "Name": "Bedroom" },
      { "Id": 2, "Name": "Server cabinet" }
    ],
    "Sensors": [
      { "Id": 1, "PlaceId": "1", "Name": "DHT11/22 2", "Uri": "http://192.168.1.120/" },
      { "Id": 2, "PlaceId": "2", "Name": "DHT11/22 1", "Uri": "http://192.168.1.120/" },
      { "Id": 3, "PlaceId": "2", "Name": "BMP085/180", "Uri": "http://192.168.1.120/", "Except": "Temperature" }
    ]
  }

}
